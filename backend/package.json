  "license": "ISC",
  "dependencies": {
    "express": "^4.17.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.2",
    "concurrently": "^5.1.0",
    "nodemon": "^2.0.2",
    "tslint": "^6.0.0",
    "typescript": "^3.7.5"
  }
}
